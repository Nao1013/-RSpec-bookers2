<div class="cotainer">
    <div class="col-8">
<table>
    <thead>
        <tr>
            <th>image</th>
            <th>name</th>
            <th colspan="3"></th>
        </tr>
    </thead>
    <tbody>
        <% @users.each do |user| %>
        <tr>
            <td><%= user.profile_image %></td>
            <td><%= user.name %></td>
            <td><%= link_to "Show", user_path(@user.id) %></td>
        </tr>
        <% end %>
    </tbody>
</table>
</div>
</div>